package com.ustglobal.collectionframework.list;

import java.util.ArrayList;

public class TestA {

	public static void main(String[] args) {
		
		
		
		ArrayList a1 = new ArrayList();
		a1.add(12);
		a1.add(32);
		a1.add("chinni");
		a1.add(true);
		a1.add(null);
		a1.add('a');
		
		Object o1 = a1.get(2);
		System.out.println(o1);
		
		System.out.println("========================");
		
		for(int i = 0 ; i < 6 ; i++) {
			
			Object o = a1.get(i);
			System.out.println(o);
		}
		
		
		
	}
	
	
	
	
	
	
	
	
	
	
	
	
}
